&inputgw

  ! config of the scf run
  prefix       = 'licl',
  outdir       = './tmp'

  ! control parameters
  ldisp        = .true.
  multishift   = .true.
  double_grid  = .false.
  do_epsil     = .true.

  ! the grid used for the linear response
  nq1          = 2
  nq2          = 2
  nq3          = 2

