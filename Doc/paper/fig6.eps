   0    ydim 2 inside mul sub neg  rlineto
   xdim 2 inside mul sub neg   0  rlineto
   closepath clip } def
  
 /ticks
  % draws ticks of height tickheight and linewidth      ticklinewidth
  % to divide the axes in nxdiv and nydiv divisions,
  % and subticks of height tickheight/2                 to divide each interval
  % in nxsubdiv and nysubdiv subintervals
  { ticklinewidth setlinewidth newpath
  xtick1s xinc nxsubdiv mul  sub y0 moveto
    0 tickheight rlineto
    0 ydim tickheight 2 mul sub rmoveto
    0 tickheight rlineto
    0 ydim neg rmoveto
  1 1 nxdiv 1 add {
    1 1 nxsubdiv 1 sub {
     xinc 0 rmoveto 0 subtickheight rlineto
     0 ydim tickheight sub rmoveto 
     0 subtickheight rlineto
     0 ydim neg rmoveto } for
    xinc 0 rmoveto 0 tickheight rlineto
    0 ydim tickheight 2 mul sub rmoveto
    0 tickheight 
%  rmoveto
rlineto
    0 ydim neg rmoveto } for
  x0 ytick1s yinc nysubdiv mul  sub  moveto
    tickheight 0 rlineto
    xdim tickheight 2 mul sub 0 rmoveto
    tickheight 0 rlineto  xdim neg 0 rmoveto
  1 1 nydiv 1 add {
    1 1 nysubdiv 1 sub {
     0 yinc rmoveto  subtickheight 0 rlineto
     xdim tickheight sub 0 rmoveto
     subtickheight 0 rlineto
     xdim neg 0 rmoveto } for
    0 yinc rmoveto
    tickheight 0 
   rlineto
   %rmoveto
    xdim tickheight 2 mul sub 0 rmoveto
    tickheight 0 
   rlineto
   %rmoveto
    xdim neg 0 rmoveto } for
